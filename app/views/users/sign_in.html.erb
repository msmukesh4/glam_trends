
<div id="page-wrapper" class="sign-in-wrapper">
	<div class="graphs">
		<div class="sign-in-form">
			<div class="sign-in-form-top">
				<p><span>Sign In as Admin</span></p>
			</div>
			<div class="signin">
				<% if !flash[:notice].blank? %>
			  		<div class="notice">
			    		<%= flash[:notice] %>
			  		</div>
				<% end %>
				<div class="signin-rit">
					<span class="checkbox1">
						 <label class="checkbox"><input type="checkbox" name="checkbox" checked="">Forgot Password ?</label>
					</span>
					<p><a href="#">Click Here</a> </p>
					<div class="clearfix"> </div>
				</div>
				<%= form_for(:user, :url => {:action => 'sign_in'}, :html => {:multipart => true}) do |f| %>
					<div class="log-input">
						<div class="log-input-left">
						   <!-- <input type="text" class="user" value="Yourname" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email address:';}"/> -->
						   <%= f.text_field(:email) %>
						</div>
						
						<div class="clearfix"> </div>
					</div>
					<div class="log-input">
						<div class="log-input-left">
						   <!-- <input type="password" class="lock" value="password" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email address:';}"/> -->
						   <%= f.password_field(:password) %>
						</div>
						
						<div class="clearfix"> </div>
					</div>
					<!-- <input type="submit" value="Login to your account"> -->
					<%= submit_tag("Login to your account") %>
				<% end %>
			</div>
		</div>
		<hr/><hr/>
		<%= params.inspect %>
	</div>
</div>



