<% @page_title = "Users" %>

<div id="page-wrapper">
	<div class="graphs">
		<div class="xs tabls">
			<div class="panel panel-warning" data-widget="{&quot;draggable&quot;: &quot;false&quot;}" data-widget-static="">
				<div class="panel-heading">
					<h2> User <%=@user.id %></h2>
					<div class="panel-ctrls" data-actions-container="" data-action-collapse="{&quot;target&quot;: &quot;.panel-body&quot;}"><span class="button-icon has-bg"><i class="ti ti-angle-down"></i></span></div>
				</div>
				<div class="panel-body no-padding" style="display: block;">
					<%= link_to("Edit User", {:action => 'edit', :user => @user.id}, :class => "btn-success btn") %> &nbsp; &nbsp; &nbsp; &nbsp;
					<%= link_to("Back to User List", {:action => 'index'}, :class => "btn-danger btn") %> <br/> <br/>
					<table class="table">
						<thead>
							<tr class="success">
								<th>Attributes</th>
								<th>Values</th>
							</tr>
						</thead>
						<tbody>
							
							<tr class="info">
								<th> User ID : </th>
								<td><%= @user.id %></td>
							</tr>
							<tr>
								<th>First name : </th>
								<td><%= @user.first_name %></td>
							</tr>
							<tr class="info">
								<th>Last name</th>
								<td><%= @user.last_name %></td>
							</tr>
							<tr>
								<th>Email</th>
								<td><%= @user.email %></td>
							</tr>
							<tr class="info">
								<th>Contact Number 1</th>
								<td><%= @user.contact_number1 %></td>
							</tr>
							
							<tr>
								<th>Contact Number 2 </th>
								<td><%= @user.contact_number2 %></td>
							</tr>
							
							<tr class="info">
								<th>Last Logged in</th>
								<td><%= @user.last_logged_in %></td>
							</tr>
							
							<tr>
								<th> Is first logged in</th>
								<td><%= @user.is_first_logged_in %></td>
							</tr>
							<tr class="info">
								<th>Is owner</th>
								<td><%= @user.is_owner %></td>
							</tr>
							<tr>
								<th> Loyality Discount at</th>
								<td><%= @user.loyality_discount_at %></td>
							</tr>
							<tr class="info">
								<th>Location</th>
								<td><%= @user.user_last_location %></td>
							</tr>
							<tr>
								<th> Status </th>
								<td><%= @user.status %></td>
							</tr>
							
						</tbody>
					</table>

				</div>
			</div>
		</div>

	</div>
</div>